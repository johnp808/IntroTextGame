	public void door() {
		System.out.println("            __________\n"
				+ "           |  __  __  |\n"
				+ "           | |  ||  | |\n"
				+ "           | |  ||  | |\n"
				+ "           | |__||__| |\n"
				+ "           |  __  __()|\n"
				+ "           | |  ||  | |\n"
				+ "           | |  ||  | |\n"
				+ "           | |  ||  | |\n"
				+ "           | |  ||  | |\n"
				+ "           | |__||__| |\n"
				+ "           |__________|");
	}
	
	public void shiningDoor() {
		System.out.println("         _____________ /\n"
				+ "        |  |  __  __  |-\n"
				+ "        |  | |  ||  | |`\n"
				+ "        |  | |  ||  | |\n"
				+ "        |  | |__||__| |\n"
				+ "        |  |  __  __()|\n"
				+ "        |  | |  ||  | |\n"
				+ "        |  | |  ||  | |\n"
				+ "        |  | |  ||  | |\n"
				+ "        |  | |  ||  | |\n"
				+ "        |  | |__||__| |\n"
				+ "        |__|__________|");
	}
