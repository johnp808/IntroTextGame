package levels;

import java.util.Scanner;

public class LevelOne {
	Scanner kb = new Scanner(System.in);
	String name;

	public void setup() {
		System.out.println("Welcome To Your New Z-Island Paradise Text Based Adventure!");
		System.out.println("First I Need To Get To Know You, What's Your Name?");
		name = kb.nextLine();
		System.out.println("Oh... I See So Your Name Is " + name);
	}
}
