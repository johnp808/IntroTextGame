package levels;

import java.util.Scanner;

public class LevelOne {
	Scanner kb = new Scanner(System.in);
	String name;

	public void setup() {
		System.out.println("Welcome To Your New Z-Island Paradise Text Based Adventure! \n");
		System.out.println("First I Need To Get To Know You, What's Your Name?");
		name = kb.nextLine();
		System.out.println("\n");
		System.out.println("Oh... I See So Your Name Is " + name + "\n");
		System.out.println("Nice To Meet You " + name + "! My Name Is Naz! \n");
		System.out.println("We Are Going To Be Stuck Together For A While So It's Best We Get To Know Each Other Okay?");
	}
}
